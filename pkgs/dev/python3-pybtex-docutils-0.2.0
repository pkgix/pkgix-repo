
version=0.2.0
description="pybtex-docutils"
website="https://pypi.python.org/pypi/pybtex-docutils/"
license=('MIT')
depends=('dev/python3' 'dev/python3-pybtex')

isinstalled() {
	python3 -c 'import pybtex-docutils' &>/dev/null
}

iscompat() { return 0; }

build() {
	fetch_extract \
		"http://pypi.python.org/packages/source/p/pybtex-docutils/pybtex-docutils-${version}.zip" \
		"9e7697788b0cd34f5ae66b686fa0836a33b4367e75d0fbe16500b1e1f2485c7a"
}

installenv() {
	cd "pybtex-docutils-${version}"
	python3 setup.py install --prefix="${prefix}/usr" --root="${destdir}" --optimize=1
}

